# Database
DATABASE_URL="postgresql://user:password@localhost:5432/mydb"

# Resend (optional in dev - uses console.log)
RESEND_API_KEY="re_..."
AUTH_EMAIL_FROM="Auth <auth@yourdomain.com>"

# Upstash Redis (optional in dev - uses in-memory)
